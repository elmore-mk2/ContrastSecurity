{
    "name": "PetClinicTest",
    "description": "FindOwner",
    "source": {
        "type": "GITHUB",
        "location": "https://github.com/turbou/PetClinicDemo",
        "gitCloneDepth": 1,
        "gitSubmodulesConfig": {
            "fetchSubmodules": false
        },
        "buildspec": "testspec.yml",
        "insecureSsl": false
    },
    "artifacts": {
        "type": "NO_ARTIFACTS"
    },
    "cache": {
        "type": "LOCAL",
        "modes": [
            "LOCAL_CUSTOM_CACHE"
        ]
    },
    "environment": {
        "type": "LINUX_CONTAINER",
        "image": "aws/codebuild/amazonlinux2-x86_64-standard:3.0",
        "computeType": "BUILD_GENERAL1_SMALL",
        "environmentVariables": [],
        "privilegedMode": false,
        "imagePullCredentialsType": "CODEBUILD"
    },
    "serviceRole": "arn:aws:iam::310199975805:role/service-role/CodeBuildServiceRole-Demo",
    "timeoutInMinutes": 60,
    "queuedTimeoutInMinutes": 480,
    "logsConfig": {
        "cloudWatchLogs": {
            "status": "ENABLED",
            "groupName": "/demo/petclinic",
            "streamName": "test"
        },
        "s3Logs": {
            "status": "DISABLED",
            "encryptionDisabled": true
        }
    },
    "buildBatchConfig": {
        "serviceRole": "arn:aws:iam::310199975805:role/service-role/CodeBuildServiceRole-Demo", 
        "combineArtifacts": true,
        "restrictions": {
            "maximumBuildsAllowed": 1,
            "computeTypesAllowed": [
                "BUILD_GENERAL1_SMALL"  
            ]   
        },  
        "timeoutInMins": 10
    }
}
